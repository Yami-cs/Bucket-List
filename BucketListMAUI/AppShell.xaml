<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="BucketListMAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:BucketListMAUI"
    xmlns:views="clr-namespace:BucketListMAUI"
    xmlns:view="clr-namespace:BucketListMAUI.View"
    Shell.BackgroundColor="#80FFFFFF"
    Shell.FlyoutBehavior="Flyout"
    FlyoutBackgroundColor="{DynamicResource Secondary}"
    Shell.TitleColor="{DynamicResource Accent}"
    Shell.ForegroundColor="{DynamicResource Accent}">
    <TabBar>
        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate view:MainPage}"
        Route="MainPage" />

    </TabBar>
</Shell>
